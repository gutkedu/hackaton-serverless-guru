{
  "name": "container-application",
  "version": "1.0.0",
  "description": "Container-based application sharing codebase with Lambda functions",
  "type": "module",
  "scripts": {
    "build": "node esbuild.container.mjs",
    "start": "node --enable-source-maps dist/container/server.js",
    "dev": "tsx watch src/container/server.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.16.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.798.0",
    "@aws-sdk/client-dynamodb": "^3.799.0",
    "@aws-sdk/client-eventbridge": "^3.798.0",
    "@aws-sdk/client-secrets-manager": "^3.777.0",
    "@aws-sdk/lib-dynamodb": "^3.799.0",
    "@gomomento/sdk": "^1.111.0",
    "aws-jwt-verify": "^4.0.1",
    "fastify": "^5.3.2",
    "fastify-plugin": "^4.5.1",
    "ksuid": "^3.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "esbuild": "^0.25.1",
    "esbuild-node-externals": "^1.13.0",
    "eslint": "^9.22.0",
    "tsx": "^4.7.1",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "@types/aws-lambda": "^8.10.147"
  }
}
