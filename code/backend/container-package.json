{
  "name": "container-application",
  "version": "1.0.0",
  "description": "Container-based application sharing codebase with Lambda functions",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node --enable-source-maps dist/container/index.js",
    "dev": "tsx watch src/container/index.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@aws-lambda-powertools/idempotency": "^2.19.0",
